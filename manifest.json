{
  "manifest_version": 3,
  "name": "Tinder Assistant",
  "version": "1.0",
  "description": "Automate Tinder messages with LLMs",
  "permissions": ["storage"],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.tinder.com/*"],
      "js": ["content.js"]
    }
  ]
}
